<?xml version="1.0"?>
<ruleset name="Dikki Config">
    <description>PHPCS configuration for the Dikki Config project</description>
    <rule ref="PSR12"/>
    <file>src</file>
    <file>tests</file>
    <exclude-pattern>*/vendor/*</exclude-pattern>
    <exclude-pattern>*/node_modules/*</exclude-pattern>
    <exclude-pattern>*/cache/*</exclude-pattern>
    <exclude-pattern>*/logs/*</exclude-pattern>
</ruleset>
