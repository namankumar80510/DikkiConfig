{
    "name": "dikki/config",
    "description": "Write and Parse Config Files in YAML, JSON, INI or PHP Array format",
    "keywords": [
        "config",
        "yaml",
        "json",
        "ini",
        "php",
        "array",
        "dotenv"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Naman Kumar",
            "email": "namankumar80510@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "nette/neon": "^3.4",
        "symfony/yaml": "7.0.*"
    },
    "require-dev": {
        "nette/tester": "^2.5",
        "phpstan/phpstan": "^1.12"
    },
    "autoload": {
      "psr-4": {
        "Dikki\\Config\\": "src/"
      }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
